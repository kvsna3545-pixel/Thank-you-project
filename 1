               s.SoundId = "rbxassetid://6129291390"
               s.Volume = 99999999999999999999999999999999999999999999
               s.Looped = true
               s:Play()
               print("Screamed "..v.Name)
               while wait(9) do
   
                  newimage.Parent:Destroy()
   
               end
            end)
         end
      end
   end)
   
end
