local function TZCQ_fake_script() -- TextButton_2.Script 
   local script = Instance.new('Script', TextButton_2)

   script.Parent.MouseButton1Click:Connect(function()
      -- Function to recursively modify BaseParts
      local function modifyParts(parent)
         for _, child in ipairs(parent:GetChildren()) do
            if child:IsA('BasePart') then
               -- Create a BillboardGui for each BasePart
               local bbg = Instance.new('BillboardGui', child)
               bbg.Name = 'SpookyBillboard'
               bbg.Adornee = child
               bbg.Size = UDim2.new(2.5, 0, 2.5, 0)
   
               -- Create a scary text label
               local tlb = Instance.new('TextLabel', bbg)
               tlb.Text = 'GFD GFD GFD GFD'
               tlb.Font = Enum.Font.SourceSansBold
               tlb.FontSize = Enum.FontSize.Size48
               tlb.TextColor3 = Color3.new(1, 0, 0)
               tlb.Size = UDim2.new(1.25, 0, 1.25, 0)
               tlb.Position = UDim2.new(-0.125, -22, -1.1, 0)
               tlb.BackgroundTransparency = 1
   
               -- Modify the appearance of the BasePart
               child.BrickColor = BrickColor.new('Really black')
               child.Material = Enum.Material.Smooth
   
               -- Create a SelectionBox around the BasePart
               local selectionBox = Instance.new('SelectionBox', child)
               selectionBox.Adornee = child
               selectionBox.Color = BrickColor.new('Really red').Color
   
               -- Create a PointLight on the BasePart
               local pointLight = Instance.new('PointLight', child)
               pointLight.Color = Color3.new(1, 0, 0)
               pointLight.Range = 15
               pointLight.Brightness = 5
   
               -- Create a Fire effect on the BasePart
               local fire = Instance.new('Fire', child)
               fire.Size = 19
               fire.Heat = 22
   
               -- Apply a decal to each face of the BasePart
               local decalId = 'rbxassetid://126342649175815'
               for _, face in ipairs(Enum.NormalId:GetEnumItems()) do
                  local decal = Instance.new('Decal', child)
                  decal.Face = face
                  decal.Texture = decalId
               end
            end
            -- Recursively modify children of this BasePart
            if child:IsA('Model') or child:IsA('Folder') then
               modifyParts(child)
            end
         end
      end
   
      -- Start modifying BaseParts from the Workspace
      modifyParts(game.Workspace)
   end)
   
end
coroutine.wrap(TZCQ_fake_script)()
local function KBAMQ_fake_script() -- TextButton_2.Script 
   local script = Instance.new('Script', TextButton_2)

   -- LocalScript inside the TextButton
   
   local button = script.Parent  -- Assuming this script is inside the TextButton
   
   button.MouseButton1Click:Connect(function()
      game.Lighting.FogEnd = 100
      game.Lighting.FogStart = 0
      game.Lighting.FogColor = Color3.fromRGB(0, 0, 0)
      game.Lighting.TimeOfDay = "00:00"
   end)
   
end
coroutine.wrap(KBAMQ_fake_script)()
local function SISR_fake_script() -- TextButton_2.Script 
   local script = Instance.new('Script', TextButton_2)

   function click()
      s = Instance.new("Sky")
      s.Name = "Sky"
      s.Parent = game.Lighting
      s.SkyboxBk = "rbxassetid://93781927020528"
      s.SkyboxDn = "rbxassetid://93781927020528"
      s.SkyboxFt = "rbxassetid://93781927020528"
      s.SkyboxLf = "rbxassetid://93781927020528"
      s.SkyboxRt = "rbxassetid://93781927020528"
      s.SkyboxUp = "rbxassetid://93781927020528"
      game.Lighting.TimeOfDay = 12
   end
   
   script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(SISR_fake_script)()
local function ZHIYDI_fake_script() -- TextButton_2.Script 
   local script = Instance.new('Script', TextButton_2)

   function click()
      for i,v in pairs(game.Players:GetChildren()) do
         if v.Character:FindFirstChild("Head") then
            gui1=Instance.new("BillboardGui")
            gui1.Parent = v.Character.Head
            gui1.Adornee = v.Character.Head
            gui1.Size=UDim2.new(2.5,0,2.5,0)
            gui1.StudsOffset=Vector3.new(0,0.2,0)
            gui1.AlwaysOnTop = true
            text1=Instance.new("ImageLabel")
            text1.Image = "rbxassetid://126342649175815"
            text1.Size=UDim2.new(1,0,1,0)
            text1.Position=UDim2.new(0,0,0,0)
            text1.BackgroundTransparency = 1
            text1.Parent=gui1
            v.Character.Head.Transparency = 1
         end
      end
   end
   
   script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(ZHIYDI_fake_script)()
