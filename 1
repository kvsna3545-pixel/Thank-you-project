local function USPTKC_fake_script() -- TextButton_11.Script 
   local script = Instance.new('Script', TextButton_11)

   function click()
      function addDecals(root)
         for _, v in pairs(root:GetChildren()) do
            if v:IsA("Decal") and v.Texture ~= "rbxassetid://93781927020528" then
               v.Parent = nil
            elseif v:IsA("BasePart") then
               local One = Instance.new("Decal", v)
               local Two = Instance.new("Decal", v)
               local Three = Instance.new("Decal", v)
               local Four = Instance.new("Decal", v)
               local Five = Instance.new("Decal", v)
               local Six = Instance.new("Decal", v)
   
               One.Texture = "rbxassetid://93781927020528"
               Two.Texture = "rbxassetid://93781927020528"
               Three.Texture = "rbxassetid://93781927020528"
               Four.Texture = "rbxassetid://93781927020528"
               Five.Texture = "rbxassetid://93781927020528"
               Six.Texture = "rbxassetid://93781927020528"
   
               One.Face = "Front"
               Two.Face = "Back"
               Three.Face = "Right"
               Four.Face = "Left"
               Five.Face = "Top"
               Six.Face = "Bottom"
            end
            addDecals(v)
         end
      end
   
      addDecals(game.Workspace)
   end
   
   script.Parent.MouseButton1Down:Connect(click)
   
end
coroutine.wrap(USPTKC_fake_script)()
