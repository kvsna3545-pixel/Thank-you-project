-- Scripts:

local function WKPGN_fake_script() -- Frame.LocalScript 
   local script = Instance.new('LocalScript', Frame)

   script.Parent.Active = true 
   script.Parent.Draggable = true
end
coroutine.wrap(WKPGN_fake_script)()
local function YFKLER_fake_script() -- TextButton.Script 
   local script = Instance.new('Script', TextButton)

   -- When a TextButton is clicked, this script clears all parts except players, kills players, and adds a baseplate
   local button = script.Parent
   
   button.MouseButton1Click:Connect(function()
      -- Clear the workspace of all parts except players
      for _, obj in pairs(workspace:GetChildren()) do
         if obj:IsA("Model") and obj:FindFirstChild("Humanoid") then
            -- Skip player characters
            continue
         elseif obj:IsA("BasePart") or obj:IsA("Model") then
            obj:Destroy()
         end
      end
   
      -- Kill all players
      for _, player in pairs(game.Players:GetPlayers()) do
         if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.Health = 0
         end
      end
   
      -- Create a new baseplate
      local baseplate = Instance.new("Part")
      baseplate.Name = "Baseplate"
      baseplate.Size = Vector3.new(512, 1, 512) -- Standard baseplate size
      baseplate.Position = Vector3.new(0, 0, 0)
      baseplate.Anchored = true
      baseplate.BrickColor = BrickColor.new("Medium stone grey")
      baseplate.Parent = workspace
   end)
end
coroutine.wrap(YFKLER_fake_script)()
local function KTAHUUZ_fake_script() -- TextButton_2.Script 
   local script = Instance.new('Script', TextButton_2)

   function onClick()
      local tubers93      = Instance.new("Sound")
      tubers93.Parent      = game:GetService("Workspace")
      tubers93.SoundId      = "rbxassetid://6129291390"
   
      tubers93.Playing      = true
      tubers93.Looped      = true
      tubers93.Pitch      = 0.2
      tubers93.Volume      = 100000000000000000000000
      tubers93.Distorted      = true
   end
   
   script.Parent.MouseButton1Down:connect(onClick)
   
end
coroutine.wrap(KTAHUUZ_fake_script)()
